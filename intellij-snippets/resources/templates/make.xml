<templateSet group="make">
  <template description="" name="base" toReformat="true" toShortenFQNames="true" value=".PHONY: clean, mrproperCC = gccCFLAGS = -g -Wallall: $1%.o: %.c$(CC) $(CFLAGS) -c -o $@ $&lt;$1$: $1.o$(CC) $(CFLAGS) -o $@ $+clean:rm -f *.o core.*mrproper: cleanrm -f $1">
    <variable alwaysStopAt="true" defaultValue="&quot;out&quot;" expression="" name="1"/>
    <context>
      <option name="MAKE" value="true"/>
    </context>
  </template>
  <template description="" name="add" toReformat="true" toShortenFQNames="true" value="$1$: $1.o$(CC) $(CFLAGS) -o $@ $+">
    <variable alwaysStopAt="true" defaultValue="&quot;out&quot;" expression="" name="1"/>
    <context>
      <option name="MAKE" value="true"/>
    </context>
  </template>
  <template description="" name="print" toReformat="true" toShortenFQNames="true" value="print-%: ; @echo $*=$($*)">
    <context>
      <option name="MAKE" value="true"/>
    </context>
  </template>
  <template description="" name="if" toReformat="true" toShortenFQNames="true" value="ifeq ($1$, $2$)$END$endif">
    <variable alwaysStopAt="true" defaultValue="&quot;cond0&quot;" expression="" name="1"/>
    <variable alwaysStopAt="true" defaultValue="&quot;cond1&quot;" expression="" name="2"/>
    <context>
      <option name="MAKE" value="true"/>
    </context>
  </template>
  <template description="" name="ife" toReformat="true" toShortenFQNames="true" value="ifeq ($1$, $2$)$3$else$END$endif">
    <variable alwaysStopAt="true" defaultValue="&quot;cond0&quot;" expression="" name="1"/>
    <variable alwaysStopAt="true" defaultValue="&quot;cond1&quot;" expression="" name="2"/>
    <context>
      <option name="MAKE" value="true"/>
    </context>
  </template>
  <template description="" name="el" toReformat="true" toShortenFQNames="true" value="else$END$">
    <context>
      <option name="MAKE" value="true"/>
    </context>
  </template>
  <template description="" name="default" toReformat="true" toShortenFQNames="true" value=".DEFAULT_GOAL := $1$">
    <context>
      <option name="MAKE" value="true"/>
    </context>
  </template>
</templateSet>
