<templateSet group="javascript-mocha">
  <template description="" name="des" toReformat="true" toShortenFQNames="true" value="describe('$1$', () =&gt; {$END$});">
    <context>
      <option name="JAVASCRIPT" value="true"/>
    </context>
  </template>
  <template description="" name="it" toReformat="true" toShortenFQNames="true" value="it('$1$', () =&gt; {$END$});">
    <context>
      <option name="JAVASCRIPT" value="true"/>
    </context>
  </template>
  <template description="" name="xit" toReformat="true" toShortenFQNames="true" value="xit('$1$', () =&gt; {$END$});">
    <context>
      <option name="JAVASCRIPT" value="true"/>
    </context>
  </template>
  <template description="" name="exp" toReformat="true" toShortenFQNames="true" value="expect($1$)$END$;">
    <variable alwaysStopAt="true" defaultValue="&quot;})${0&quot;" expression="" name="1"/>
    <context>
      <option name="JAVASCRIPT" value="true"/>
    </context>
  </template>
  <template description="" name="expe" toReformat="true" toShortenFQNames="true" value="expect($1$).to.equal($END$);">
    <variable alwaysStopAt="true" defaultValue="&quot;}).to.equal(${0&quot;" expression="" name="1"/>
    <context>
      <option name="JAVASCRIPT" value="true"/>
    </context>
  </template>
</templateSet>
