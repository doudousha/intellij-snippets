<templateSet group="fortran">
  <template description="" name="impl" toReformat="true" toShortenFQNames="true" value="implicit none$0">
    <context>
      <option name="FORTRAN" value="true"/>
    </context>
  </template>
  <template description="" name="prog" toReformat="true" toShortenFQNames="true" value="program $1$$0end program $1">
    <variable alwaysStopAt="true" defaultValue="&quot;main&quot;" expression="" name="1"/>
    <context>
      <option name="FORTRAN" value="true"/>
    </context>
  </template>
  <template description="" name="mod" toReformat="true" toShortenFQNames="true" value="module $1$$0end module $1">
    <variable alwaysStopAt="true" defaultValue="&quot;modulename&quot;" expression="" name="1"/>
    <context>
      <option name="FORTRAN" value="true"/>
    </context>
  </template>
  <template description="" name="proc" toReformat="true" toShortenFQNames="true" value="procedure $1$$END$end procedure $1">
    <variable alwaysStopAt="true" defaultValue="&quot;name&quot;" expression="" name="1"/>
    <context>
      <option name="FORTRAN" value="true"/>
    </context>
  </template>
  <template description="" name="iface" toReformat="true" toShortenFQNames="true" value="interface $1$$END$end interface $1">
    <variable alwaysStopAt="true" defaultValue="&quot;name&quot;" expression="" name="1"/>
    <context>
      <option name="FORTRAN" value="true"/>
    </context>
  </template>
  <template description="" name="doc" toReformat="true" toShortenFQNames="true" value="! &quot;&quot;&quot;&quot;&quot;&quot;&quot;&quot;&quot;&quot;&quot;&quot;&quot;&quot;&quot;&quot;&quot;&quot;&quot;&quot;&quot;&quot;&quot;&quot;&quot;&quot;&quot;&quot;&quot;&quot;&quot;&quot;&quot;&quot;&quot;&quot;&quot;&quot;&quot;&quot;&quot;&quot;&quot;&quot;&quot;&quot;&quot;&quot;&quot;&quot;&quot;&quot;&quot;&quot;&quot;&quot;&quot;&quot;&quot;&quot;&quot;&quot;&quot;&quot;&quot;&quot;&quot;&quot;&quot;&quot;&quot;&quot;&quot;&quot;! File: $2$! Author: `g:snips_author`! Email: `g:snips_email`! Github: `g:snips_github`! Description: $1! &quot;&quot;&quot;&quot;&quot;&quot;&quot;&quot;&quot;&quot;&quot;&quot;&quot;&quot;&quot;&quot;&quot;&quot;&quot;&quot;&quot;&quot;&quot;&quot;&quot;&quot;&quot;&quot;&quot;&quot;&quot;&quot;&quot;&quot;&quot;&quot;&quot;&quot;&quot;&quot;&quot;&quot;&quot;&quot;&quot;&quot;&quot;&quot;&quot;&quot;&quot;&quot;&quot;&quot;&quot;&quot;&quot;&quot;&quot;&quot;&quot;&quot;&quot;&quot;&quot;&quot;&quot;&quot;&quot;&quot;&quot;&quot;&quot;&quot;$0">
    <variable alwaysStopAt="true" defaultValue="&quot;`vim_snippets#Filename('$1')`&quot;" expression="" name="2"/>
    <context>
      <option name="FORTRAN" value="true"/>
    </context>
  </template>
  <template description="" name="dox" toReformat="true" toShortenFQNames="true" value="!&gt; @brief $1$!&gt; $2$!&gt; @author `g:snips_author`$END$">
    <context>
      <option name="FORTRAN" value="true"/>
    </context>
  </template>
  <template description="" name="bool" toReformat="true" toShortenFQNames="true" value="logical :: $0">
    <context>
      <option name="FORTRAN" value="true"/>
    </context>
  </template>
  <template description="" name="int" toReformat="true" toShortenFQNames="true" value="integer :: $0">
    <context>
      <option name="FORTRAN" value="true"/>
    </context>
  </template>
  <template description="" name="real" toReformat="true" toShortenFQNames="true" value="real :: $0">
    <context>
      <option name="FORTRAN" value="true"/>
    </context>
  </template>
  <template description="" name="double" toReformat="true" toShortenFQNames="true" value="double precision :: $0">
    <context>
      <option name="FORTRAN" value="true"/>
    </context>
  </template>
  <template description="" name="str" toReformat="true" toShortenFQNames="true" value="character(len=$1$) :: $END$">
    <variable alwaysStopAt="true" defaultValue="&quot;*&quot;" expression="" name="1"/>
    <context>
      <option name="FORTRAN" value="true"/>
    </context>
  </template>
  <template description="" name="type" toReformat="true" toShortenFQNames="true" value="type($1$)$0end type">
    <variable alwaysStopAt="true" defaultValue="&quot;name&quot;" expression="" name="1"/>
    <context>
      <option name="FORTRAN" value="true"/>
    </context>
  </template>
  <template description="" name="const" toReformat="true" toShortenFQNames="true" value="$1$, parameter :: $2 = $0">
    <variable alwaysStopAt="true" defaultValue="&quot;type&quot;" expression="" name="1"/>
    <context>
      <option name="FORTRAN" value="true"/>
    </context>
  </template>
  <template description="" name="arr" toReformat="true" toShortenFQNames="true" value="$1$, $2$dimension($3$) :: $0">
    <variable alwaysStopAt="true" defaultValue="&quot;type&quot;" expression="" name="1"/>
    <variable alwaysStopAt="true" defaultValue="&quot;allocatable, &quot;" expression="" name="2"/>
    <variable alwaysStopAt="true" defaultValue="&quot;:&quot;" expression="" name="3"/>
    <context>
      <option name="FORTRAN" value="true"/>
    </context>
  </template>
  <template description="" name="intent" toReformat="true" toShortenFQNames="true" value="$1$, intent(inout) :: $0">
    <variable alwaysStopAt="true" defaultValue="&quot;type&quot;" expression="" name="1"/>
    <context>
      <option name="FORTRAN" value="true"/>
    </context>
  </template>
  <template description="" name="/" toReformat="true" toShortenFQNames="true" value="(/ $1 /) $2$ $0">
    <variable alwaysStopAt="true" defaultValue="&quot;,&amp;&quot;" expression="" name="2"/>
    <context>
      <option name="FORTRAN" value="true"/>
    </context>
  </template>
  <template description="" name="if" toReformat="true" toShortenFQNames="true" value="if ($1$) then$0end if">
    <variable alwaysStopAt="true" defaultValue="&quot;condition&quot;" expression="" name="1"/>
    <context>
      <option name="FORTRAN" value="true"/>
    </context>
  </template>
  <template description="" name="case" toReformat="true" toShortenFQNames="true" value="select case ($1$)case ($2)case default$3end select $0">
    <variable alwaysStopAt="true" defaultValue="&quot;expr&quot;" expression="" name="1"/>
    <context>
      <option name="FORTRAN" value="true"/>
    </context>
  </template>
  <template description="" name="do" toReformat="true" toShortenFQNames="true" value="do $1$ = $2$, $3$, $4$$0end do">
    <variable alwaysStopAt="true" defaultValue="&quot;i&quot;" expression="" name="1"/>
    <variable alwaysStopAt="true" defaultValue="&quot;start&quot;" expression="" name="2"/>
    <variable alwaysStopAt="true" defaultValue="&quot;end&quot;" expression="" name="3"/>
    <variable alwaysStopAt="true" defaultValue="&quot;incr&quot;" expression="" name="4"/>
    <context>
      <option name="FORTRAN" value="true"/>
    </context>
  </template>
  <template description="" name="dow" toReformat="true" toShortenFQNames="true" value="do while ($1$)$2end do">
    <variable alwaysStopAt="true" defaultValue="&quot;condition&quot;" expression="" name="1"/>
    <context>
      <option name="FORTRAN" value="true"/>
    </context>
  </template>
  <template description="" name="sub" toReformat="true" toShortenFQNames="true" value="subroutine $1$($2)$0end subroutine $1">
    <variable alwaysStopAt="true" defaultValue="&quot;name&quot;" expression="" name="1"/>
    <context>
      <option name="FORTRAN" value="true"/>
    </context>
  </template>
  <template description="" name="func" toReformat="true" toShortenFQNames="true" value="function $1$($2) result($3)$0end function $1">
    <variable alwaysStopAt="true" defaultValue="&quot;name&quot;" expression="" name="1"/>
    <context>
      <option name="FORTRAN" value="true"/>
    </context>
  </template>
  <template description="" name="pr" toReformat="true" toShortenFQNames="true" value="write(*,*) $0">
    <context>
      <option name="FORTRAN" value="true"/>
    </context>
  </template>
  <template description="" name="read" toReformat="true" toShortenFQNames="true" value="read(unit = $1$, file = $2$, iostat = $3$) $0">
    <variable alwaysStopAt="true" defaultValue="&quot;fp&quot;" expression="" name="1"/>
    <variable alwaysStopAt="true" defaultValue="&quot;filename&quot;" expression="" name="2"/>
    <variable alwaysStopAt="true" defaultValue="&quot;ierr&quot;" expression="" name="3"/>
    <context>
      <option name="FORTRAN" value="true"/>
    </context>
  </template>
  <template description="" name="write" toReformat="true" toShortenFQNames="true" value="write(unit = $1$, file = $2$, iostat = $3$) $0">
    <variable alwaysStopAt="true" defaultValue="&quot;fp&quot;" expression="" name="1"/>
    <variable alwaysStopAt="true" defaultValue="&quot;filename&quot;" expression="" name="2"/>
    <variable alwaysStopAt="true" defaultValue="&quot;ierr&quot;" expression="" name="3"/>
    <context>
      <option name="FORTRAN" value="true"/>
    </context>
  </template>
  <template description="" name="open" toReformat="true" toShortenFQNames="true" value="open(unit = $1$, file = $2$, status = $3$, iostat = $4$) $0">
    <variable alwaysStopAt="true" defaultValue="&quot;fp&quot;" expression="" name="1"/>
    <variable alwaysStopAt="true" defaultValue="&quot;filename&quot;" expression="" name="2"/>
    <variable alwaysStopAt="true" defaultValue="&quot;unknown&quot;" expression="" name="3"/>
    <variable alwaysStopAt="true" defaultValue="&quot;ierr&quot;" expression="" name="4"/>
    <context>
      <option name="FORTRAN" value="true"/>
    </context>
  </template>
</templateSet>
