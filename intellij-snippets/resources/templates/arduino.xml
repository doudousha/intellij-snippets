<templateSet group="arduino">
  <template description="" name="setup" toReformat="true" toShortenFQNames="true" value="void setup(){Serial.begin($1$);$2$}">
    <variable alwaysStopAt="true" defaultValue="&quot;9600&quot;" expression="" name="1"/>
    <context>
      <option name="ARDUINO" value="true"/>
    </context>
  </template>
  <template description="" name="loop" toReformat="true" toShortenFQNames="true" value="void loop(){$END$}">
    <context>
      <option name="ARDUINO" value="true"/>
    </context>
  </template>
  <template description="" name="inc" toReformat="true" toShortenFQNames="true" value="#include &lt;$1$.h&gt;">
    <context>
      <option name="ARDUINO" value="true"/>
    </context>
  </template>
  <template description="" name="def" toReformat="true" toShortenFQNames="true" value="#define $1$">
    <context>
      <option name="ARDUINO" value="true"/>
    </context>
  </template>
  <template description="" name="if" toReformat="true" toShortenFQNames="true" value="if ($1$) {$2$}">
    <variable alwaysStopAt="true" defaultValue="&quot;/* condition */&quot;" expression="" name="1"/>
    <context>
      <option name="ARDUINO" value="true"/>
    </context>
  </template>
  <template description="" name="el" toReformat="true" toShortenFQNames="true" value="else {$1$}">
    <context>
      <option name="ARDUINO" value="true"/>
    </context>
  </template>
  <template description="" name="elif" toReformat="true" toShortenFQNames="true" value="else if ($1$) {$2$}">
    <variable alwaysStopAt="true" defaultValue="&quot;/* condition */&quot;" expression="" name="1"/>
    <context>
      <option name="ARDUINO" value="true"/>
    </context>
  </template>
  <template description="" name="ifi" toReformat="true" toShortenFQNames="true" value="if ($1$) $2$;">
    <variable alwaysStopAt="true" defaultValue="&quot;/* condition */&quot;" expression="" name="1"/>
    <context>
      <option name="ARDUINO" value="true"/>
    </context>
  </template>
  <template description="" name="switch" toReformat="true" toShortenFQNames="true" value="switch ($1$) {case $2$:$3$$4$$5$default:$6$}">
    <variable alwaysStopAt="true" defaultValue="&quot;/* variable */&quot;" expression="" name="1"/>
    <variable alwaysStopAt="true" defaultValue="&quot;/* variable case */&quot;" expression="" name="2"/>
    <variable alwaysStopAt="true" defaultValue="&quot;break;&quot;" expression="" name="4"/>
    <context>
      <option name="ARDUINO" value="true"/>
    </context>
  </template>
  <template description="" name="case" toReformat="true" toShortenFQNames="true" value="case $1$:$2$$3$">
    <variable alwaysStopAt="true" defaultValue="&quot;/* variable case */&quot;" expression="" name="1"/>
    <variable alwaysStopAt="true" defaultValue="&quot;break;&quot;" expression="" name="3"/>
    <context>
      <option name="ARDUINO" value="true"/>
    </context>
  </template>
  <template description="" name="for" toReformat="true" toShortenFQNames="true" value="for (int $2$ = 0; $2 &lt; $1$; $2$3$) {$4$}">
    <variable alwaysStopAt="true" defaultValue="&quot;i&quot;" expression="" name="2"/>
    <variable alwaysStopAt="true" defaultValue="&quot;count&quot;" expression="" name="1"/>
    <variable alwaysStopAt="true" defaultValue="&quot;++&quot;" expression="" name="3"/>
    <context>
      <option name="ARDUINO" value="true"/>
    </context>
  </template>
  <template description="" name="forr" toReformat="true" toShortenFQNames="true" value="for (int $1$ = $2$; $3$; $1$4$) {$5$}">
    <variable alwaysStopAt="true" defaultValue="&quot;i&quot;" expression="" name="1"/>
    <variable alwaysStopAt="true" defaultValue="&quot;0&quot;" expression="" name="2"/>
    <variable alwaysStopAt="true" defaultValue="&quot;$1 &lt; 10&quot;" expression="" name="3"/>
    <variable alwaysStopAt="true" defaultValue="&quot;++&quot;" expression="" name="4"/>
    <context>
      <option name="ARDUINO" value="true"/>
    </context>
  </template>
  <template description="" name="wh" toReformat="true" toShortenFQNames="true" value="while ($1$) {$2$}">
    <variable alwaysStopAt="true" defaultValue="&quot;/* condition */&quot;" expression="" name="1"/>
    <context>
      <option name="ARDUINO" value="true"/>
    </context>
  </template>
  <template description="" name="do" toReformat="true" toShortenFQNames="true" value="do {$2$} while ($1$);">
    <variable alwaysStopAt="true" defaultValue="&quot;/* condition */&quot;" expression="" name="1"/>
    <context>
      <option name="ARDUINO" value="true"/>
    </context>
  </template>
  <template description="" name="fun" toReformat="true" toShortenFQNames="true" value="$1$ $2$($3$){$4$}">
    <variable alwaysStopAt="true" defaultValue="&quot;void&quot;" expression="" name="1"/>
    <variable alwaysStopAt="true" defaultValue="&quot;function_name&quot;" expression="" name="2"/>
    <context>
      <option name="ARDUINO" value="true"/>
    </context>
  </template>
  <template description="" name="pinout" toReformat="true" toShortenFQNames="true" value="pinMode($1$, OUTPUT);">
    <context>
      <option name="ARDUINO" value="true"/>
    </context>
  </template>
  <template description="" name="pinin" toReformat="true" toShortenFQNames="true" value="pinMode($1$, INPUT);">
    <context>
      <option name="ARDUINO" value="true"/>
    </context>
  </template>
  <template description="" name="dwHigh" toReformat="true" toShortenFQNames="true" value="digitalWrite($1$, HIGH);">
    <context>
      <option name="ARDUINO" value="true"/>
    </context>
  </template>
  <template description="" name="dwLow" toReformat="true" toShortenFQNames="true" value="digitalWrite($1$, LOW);">
    <context>
      <option name="ARDUINO" value="true"/>
    </context>
  </template>
  <template description="" name="dr" toReformat="true" toShortenFQNames="true" value="digitalRead($1$);">
    <context>
      <option name="ARDUINO" value="true"/>
    </context>
  </template>
  <template description="" name="ar" toReformat="true" toShortenFQNames="true" value="analogRead($1$);">
    <context>
      <option name="ARDUINO" value="true"/>
    </context>
  </template>
  <template description="" name="aw" toReformat="true" toShortenFQNames="true" value="analogWrite($1$);">
    <context>
      <option name="ARDUINO" value="true"/>
    </context>
  </template>
  <template description="" name="sr" toReformat="true" toShortenFQNames="true" value="Serial.read();">
    <context>
      <option name="ARDUINO" value="true"/>
    </context>
  </template>
  <template description="" name="sw" toReformat="true" toShortenFQNames="true" value="Serial.write();">
    <context>
      <option name="ARDUINO" value="true"/>
    </context>
  </template>
  <template description="" name="sp" toReformat="true" toShortenFQNames="true" value="Serial.print($1$);">
    <context>
      <option name="ARDUINO" value="true"/>
    </context>
  </template>
  <template description="" name="sl" toReformat="true" toShortenFQNames="true" value="Serial.println($1$);">
    <context>
      <option name="ARDUINO" value="true"/>
    </context>
  </template>
  <template description="" name="dl" toReformat="true" toShortenFQNames="true" value="delay($1$);">
    <context>
      <option name="ARDUINO" value="true"/>
    </context>
  </template>
</templateSet>
