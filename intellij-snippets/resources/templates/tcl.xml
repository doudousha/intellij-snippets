<templateSet group="tcl">
  <template description="" name="#!" toReformat="true" toShortenFQNames="true" value="#!/usr/bin/env tclsh">
    <context>
      <option name="TCL" value="true"/>
    </context>
  </template>
  <template description="" name="pro" toReformat="true" toShortenFQNames="true" value="proc $1$ {$2$} {$END$}">
    <variable alwaysStopAt="true" defaultValue="&quot;function_name&quot;" expression="" name="1"/>
    <variable alwaysStopAt="true" defaultValue="&quot;args&quot;" expression="" name="2"/>
    <context>
      <option name="TCL" value="true"/>
    </context>
  </template>
  <template description="" name="xif" toReformat="true" toShortenFQNames="true" value="$1$? $2$ : $END$">
    <variable alwaysStopAt="true" defaultValue="&quot;expr&quot;" expression="" name="1"/>
    <variable alwaysStopAt="true" defaultValue="&quot;true&quot;" expression="" name="2"/>
    <variable alwaysStopAt="true" defaultValue="&quot;false&quot;" expression="" name="0"/>
    <context>
      <option name="TCL" value="true"/>
    </context>
  </template>
  <template description="" name="if" toReformat="true" toShortenFQNames="true" value="if {$1$} {$END$}">
    <context>
      <option name="TCL" value="true"/>
    </context>
  </template>
  <template description="" name="ife" toReformat="true" toShortenFQNames="true" value="if {$1$} {$2$} else {$END$}">
    <variable alwaysStopAt="true" defaultValue="&quot;# else...&quot;" expression="" name="0"/>
    <context>
      <option name="TCL" value="true"/>
    </context>
  </template>
  <template description="" name="eif" toReformat="true" toShortenFQNames="true" value="elseif {$1$} {$END$}">
    <context>
      <option name="TCL" value="true"/>
    </context>
  </template>
  <template description="" name="ifee" toReformat="true" toShortenFQNames="true" value="if {$1$} {$2$} elseif {$3$} {$4$} else {$END$}">
    <variable alwaysStopAt="true" defaultValue="&quot;# elsif...&quot;" expression="" name="4"/>
    <variable alwaysStopAt="true" defaultValue="&quot;# else...&quot;" expression="" name="0"/>
    <context>
      <option name="TCL" value="true"/>
    </context>
  </template>
  <template description="" name="ifc" toReformat="true" toShortenFQNames="true" value="if { [catch {$1$} $2$] } {$END$}">
    <variable alwaysStopAt="true" defaultValue="&quot;#do something...&quot;" expression="" name="1"/>
    <variable alwaysStopAt="true" defaultValue="&quot;err&quot;" expression="" name="2"/>
    <variable alwaysStopAt="true" defaultValue="&quot;# handle failure...&quot;" expression="" name="0"/>
    <context>
      <option name="TCL" value="true"/>
    </context>
  </template>
  <template description="" name="catch" toReformat="true" toShortenFQNames="true" value="catch {$1$} $2$ $END$">
    <variable alwaysStopAt="true" defaultValue="&quot;err&quot;" expression="" name="2"/>
    <variable alwaysStopAt="true" defaultValue="&quot;options&quot;" expression="" name="0"/>
    <context>
      <option name="TCL" value="true"/>
    </context>
  </template>
  <template description="" name="wh" toReformat="true" toShortenFQNames="true" value="while {$1$} {$END$}">
    <context>
      <option name="TCL" value="true"/>
    </context>
  </template>
  <template description="" name="for" toReformat="true" toShortenFQNames="true" value="for {set $2$ 0} {$$2 &lt; $1$} {$3$ $2} {$END$}">
    <variable alwaysStopAt="true" defaultValue="&quot;var&quot;" expression="" name="2"/>
    <variable alwaysStopAt="true" defaultValue="&quot;count&quot;" expression="" name="1"/>
    <variable alwaysStopAt="true" defaultValue="&quot;incr&quot;" expression="" name="3"/>
    <context>
      <option name="TCL" value="true"/>
    </context>
  </template>
  <template description="" name="fore" toReformat="true" toShortenFQNames="true" value="foreach $1$ {$2$} {$END$}">
    <variable alwaysStopAt="true" defaultValue="&quot;x&quot;" expression="" name="1"/>
    <variable alwaysStopAt="true" defaultValue="&quot;#list&quot;" expression="" name="2"/>
    <context>
      <option name="TCL" value="true"/>
    </context>
  </template>
  <template description="" name="af" toReformat="true" toShortenFQNames="true" value="after $1$ $END$">
    <variable alwaysStopAt="true" defaultValue="&quot;ms&quot;" expression="" name="1"/>
    <variable alwaysStopAt="true" defaultValue="&quot;#do something&quot;" expression="" name="0"/>
    <context>
      <option name="TCL" value="true"/>
    </context>
  </template>
  <template description="" name="afc" toReformat="true" toShortenFQNames="true" value="after cancel $END$">
    <variable alwaysStopAt="true" defaultValue="&quot;id or script&quot;" expression="" name="0"/>
    <context>
      <option name="TCL" value="true"/>
    </context>
  </template>
  <template description="" name="afi" toReformat="true" toShortenFQNames="true" value="after idle $END$">
    <variable alwaysStopAt="true" defaultValue="&quot;script&quot;" expression="" name="0"/>
    <context>
      <option name="TCL" value="true"/>
    </context>
  </template>
  <template description="" name="afin" toReformat="true" toShortenFQNames="true" value="after info $END$">
    <variable alwaysStopAt="true" defaultValue="&quot;id&quot;" expression="" name="0"/>
    <context>
      <option name="TCL" value="true"/>
    </context>
  </template>
  <template description="" name="exp" toReformat="true" toShortenFQNames="true" value="expr {$END$}">
    <variable alwaysStopAt="true" defaultValue="&quot;#expression here&quot;" expression="" name="0"/>
    <context>
      <option name="TCL" value="true"/>
    </context>
  </template>
  <template description="" name="sw" toReformat="true" toShortenFQNames="true" value="switch $1$ {$3$ {$END$}default {$2$}}">
    <variable alwaysStopAt="true" defaultValue="&quot;var&quot;" expression="" name="1"/>
    <variable alwaysStopAt="true" defaultValue="&quot;pattern 1&quot;" expression="" name="3"/>
    <variable alwaysStopAt="true" defaultValue="&quot;#do something&quot;" expression="" name="0"/>
    <variable alwaysStopAt="true" defaultValue="&quot;#do something&quot;" expression="" name="2"/>
    <context>
      <option name="TCL" value="true"/>
    </context>
  </template>
  <template description="" name="ca" toReformat="true" toShortenFQNames="true" value="$1$ {$2$}">
    <variable alwaysStopAt="true" defaultValue="&quot;pattern&quot;" expression="" name="1"/>
    <variable alwaysStopAt="true" defaultValue="&quot;#do something&quot;" expression="" name="2"/>
    <context>
      <option name="TCL" value="true"/>
    </context>
  </template>
  <template description="" name="ns" toReformat="true" toShortenFQNames="true" value="namespace eval $1$ {$END$}">
    <variable alwaysStopAt="true" defaultValue="&quot;path&quot;" expression="" name="1"/>
    <variable alwaysStopAt="true" defaultValue="&quot;#script...&quot;" expression="" name="0"/>
    <context>
      <option name="TCL" value="true"/>
    </context>
  </template>
</templateSet>
