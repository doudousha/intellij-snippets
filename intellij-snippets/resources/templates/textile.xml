<templateSet group="textile">
  <template description="" name="header" toReformat="true" toShortenFQNames="true" value="---title: $1$layout: postdate: $2$ $END$ -05:00---">
    <variable alwaysStopAt="true" defaultValue="&quot;title&quot;" expression="" name="1"/>
    <variable alwaysStopAt="true" defaultValue="&quot;date&quot;" expression="" name="2"/>
    <variable alwaysStopAt="true" defaultValue="&quot;hour:minute:second&quot;" expression="" name="0"/>
    <context>
      <option name="TEXTILE" value="true"/>
    </context>
  </template>
  <template description="" name="img" toReformat="true" toShortenFQNames="true" value="!$1$($2$):$END$!">
    <variable alwaysStopAt="true" defaultValue="&quot;url&quot;" expression="" name="1"/>
    <variable alwaysStopAt="true" defaultValue="&quot;title&quot;" expression="" name="2"/>
    <variable alwaysStopAt="true" defaultValue="&quot;link&quot;" expression="" name="0"/>
    <context>
      <option name="TEXTILE" value="true"/>
    </context>
  </template>
  <template description="" name="|" toReformat="true" toShortenFQNames="true" value="|$1$|">
    <context>
      <option name="TEXTILE" value="true"/>
    </context>
  </template>
  <template description="" name="link" toReformat="true" toShortenFQNames="true" value="&quot;$1$&quot;:$END$">
    <variable alwaysStopAt="true" defaultValue="&quot;link text&quot;" expression="" name="1"/>
    <variable alwaysStopAt="true" defaultValue="&quot;url&quot;" expression="" name="0"/>
    <context>
      <option name="TEXTILE" value="true"/>
    </context>
  </template>
  <template description="" name="(" toReformat="true" toShortenFQNames="true" value="($1$)">
    <variable alwaysStopAt="true" defaultValue="&quot;Expand acronym&quot;" expression="" name="1"/>
    <context>
      <option name="TEXTILE" value="true"/>
    </context>
  </template>
</templateSet>
