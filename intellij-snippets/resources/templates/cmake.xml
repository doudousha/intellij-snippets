<templateSet group="cmake">
  <template description="" name="init" toReformat="true" toShortenFQNames="true" value="cmake_minimum_required(version $1$)project($2$)find_package($3$)include_directories(${$3_INCLUDE_DIRS})add_subdirectory($END$)add_executable($2)target_link_libraries($2 ${$3_LIBRARIES})">
    <variable alwaysStopAt="true" defaultValue="&quot;2.8.2&quot;" expression="" name="1"/>
    <variable alwaysStopAt="true" defaultValue="&quot;ProjectName&quot;" expression="" name="2"/>
    <variable alwaysStopAt="true" defaultValue="&quot;library&quot;" expression="" name="3"/>
    <variable alwaysStopAt="true" defaultValue="&quot;src&quot;" expression="" name="0"/>
    <context>
      <option name="CMAKE" value="true"/>
    </context>
  </template>
  <template description="" name="proj" toReformat="true" toShortenFQNames="true" value="project($END$)">
    <variable alwaysStopAt="true" defaultValue="&quot;Name&quot;" expression="" name="0"/>
    <context>
      <option name="CMAKE" value="true"/>
    </context>
  </template>
  <template description="" name="min" toReformat="true" toShortenFQNames="true" value="cmake_minimum_required(version $END$)">
    <variable alwaysStopAt="true" defaultValue="&quot;2.8.2&quot;" expression="" name="0"/>
    <context>
      <option name="CMAKE" value="true"/>
    </context>
  </template>
  <template description="" name="include" toReformat="true" toShortenFQNames="true" value="include_directories(${$END$})">
    <variable alwaysStopAt="true" defaultValue="&quot;include_dir&quot;" expression="" name="0"/>
    <context>
      <option name="CMAKE" value="true"/>
    </context>
  </template>
  <template description="" name="find" toReformat="true" toShortenFQNames="true" value="find_package($1$ $END$)">
    <variable alwaysStopAt="true" defaultValue="&quot;library&quot;" expression="" name="1"/>
    <variable alwaysStopAt="true" defaultValue="&quot;REQUIRED&quot;" expression="" name="0"/>
    <context>
      <option name="CMAKE" value="true"/>
    </context>
  </template>
  <template description="" name="glob" toReformat="true" toShortenFQNames="true" value="file(glob $1$ *.$END$)">
    <variable alwaysStopAt="true" defaultValue="&quot;srcs&quot;" expression="" name="1"/>
    <variable alwaysStopAt="true" defaultValue="&quot;cpp&quot;" expression="" name="0"/>
    <context>
      <option name="CMAKE" value="true"/>
    </context>
  </template>
  <template description="" name="subdir" toReformat="true" toShortenFQNames="true" value="add_subdirectory($END$)">
    <variable alwaysStopAt="true" defaultValue="&quot;src&quot;" expression="" name="0"/>
    <context>
      <option name="CMAKE" value="true"/>
    </context>
  </template>
  <template description="" name="lib" toReformat="true" toShortenFQNames="true" value="add_library($1$ ${$END$})">
    <variable alwaysStopAt="true" defaultValue="&quot;lib&quot;" expression="" name="1"/>
    <variable alwaysStopAt="true" defaultValue="&quot;srcs&quot;" expression="" name="0"/>
    <context>
      <option name="CMAKE" value="true"/>
    </context>
  </template>
  <template description="" name="link" toReformat="true" toShortenFQNames="true" value="target_link_libraries($1$ $END$)">
    <variable alwaysStopAt="true" defaultValue="&quot;bin&quot;" expression="" name="1"/>
    <variable alwaysStopAt="true" defaultValue="&quot;somelib&quot;" expression="" name="0"/>
    <context>
      <option name="CMAKE" value="true"/>
    </context>
  </template>
  <template description="" name="bin" toReformat="true" toShortenFQNames="true" value="add_executable($1$)">
    <variable alwaysStopAt="true" defaultValue="&quot;bin&quot;" expression="" name="1"/>
    <context>
      <option name="CMAKE" value="true"/>
    </context>
  </template>
  <template description="" name="set" toReformat="true" toShortenFQNames="true" value="set($1$ $END$)">
    <variable alwaysStopAt="true" defaultValue="&quot;var&quot;" expression="" name="1"/>
    <variable alwaysStopAt="true" defaultValue="&quot;val&quot;" expression="" name="0"/>
    <context>
      <option name="CMAKE" value="true"/>
    </context>
  </template>
  <template description="" name="dep" toReformat="true" toShortenFQNames="true" value="add_dependencies($1$$END$)">
    <variable alwaysStopAt="true" defaultValue="&quot;target&quot;" expression="" name="1"/>
    <variable alwaysStopAt="true" defaultValue="&quot;dep&quot;" expression="" name="0"/>
    <context>
      <option name="CMAKE" value="true"/>
    </context>
  </template>
  <template description="" name="Ext_url" toReformat="true" toShortenFQNames="true" value="include(ExternalProject)ExternalProject_Add($1$URL $2$URL_HASH SHA1=$3$SOURCE_DIR &quot;$4$/gtest-src}&quot;BINARY_DIR &quot;$END$/gtest-build}&quot;CONFIGURE_COMMAND &quot;&quot;BUILD_COMMAND     &quot;&quot;INSTALL_COMMAND   &quot;&quot;TEST_COMMAND      &quot;&quot;)">
    <variable alwaysStopAt="true" defaultValue="&quot;googletest&quot;" expression="" name="1"/>
    <variable alwaysStopAt="true" defaultValue="&quot;http://googletest.googlecode.com/files/gtest-1.7.0.zip&quot;" expression="" name="2"/>
    <variable alwaysStopAt="true" defaultValue="&quot;f85f6d2481e2c6c4a18539e391aa4ea8ab0394af&quot;" expression="" name="3"/>
    <variable alwaysStopAt="true" defaultValue="&quot;${CMAKE_BINARY_DIR&quot;" expression="" name="4"/>
    <variable alwaysStopAt="true" defaultValue="&quot;${CMAKE_BINARY_DIR&quot;" expression="" name="0"/>
    <context>
      <option name="CMAKE" value="true"/>
    </context>
  </template>
  <template description="" name="Ext_git" toReformat="true" toShortenFQNames="true" value="include(ExternalProject)ExternalProject_Add($1$GIT_REPOSITORY    $2$GIT_TAG           $3$SOURCE_DIR        &quot;$4$/googletest-src}&quot;BINARY_DIR        &quot;$END$/googletest-build}&quot;CONFIGURE_COMMAND &quot;&quot;BUILD_COMMAND     &quot;&quot;INSTALL_COMMAND   &quot;&quot;TEST_COMMAND      &quot;&quot;)">
    <variable alwaysStopAt="true" defaultValue="&quot;googletest&quot;" expression="" name="1"/>
    <variable alwaysStopAt="true" defaultValue="&quot;https://github.com/google/googletest.git&quot;" expression="" name="2"/>
    <variable alwaysStopAt="true" defaultValue="&quot;master&quot;" expression="" name="3"/>
    <variable alwaysStopAt="true" defaultValue="&quot;${CMAKE_BINARY_DIR&quot;" expression="" name="4"/>
    <variable alwaysStopAt="true" defaultValue="&quot;${CMAKE_BINARY_DIR&quot;" expression="" name="0"/>
    <context>
      <option name="CMAKE" value="true"/>
    </context>
  </template>
  <template description="" name="props" toReformat="true" toShortenFQNames="true" value="set_target_properties($1$$2$ $3$$END$)">
    <variable alwaysStopAt="true" defaultValue="&quot;target&quot;" expression="" name="1"/>
    <variable alwaysStopAt="true" defaultValue="&quot;properties&quot;" expression="" name="2"/>
    <variable alwaysStopAt="true" defaultValue="&quot;compile_flags&quot;" expression="" name="3"/>
    <variable alwaysStopAt="true" defaultValue="&quot;&quot;-O3 -Wall -pedantic&quot;&quot;" expression="" name="0"/>
    <context>
      <option name="CMAKE" value="true"/>
    </context>
  </template>
</templateSet>
