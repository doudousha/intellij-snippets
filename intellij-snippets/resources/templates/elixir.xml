<templateSet group="elixir">
  <template description="" name="do" toReformat="true" toShortenFQNames="true" value="do$END$end">
    <context>
      <option name="ELIXIR" value="true"/>
    </context>
  </template>
  <template description="" name="if if .. do .. end" toReformat="true" toShortenFQNames="true" value="if $1$ do$END$end">
    <context>
      <option name="ELIXIR" value="true"/>
    </context>
  </template>
  <template description="" name="if: if .. do: .." toReformat="true" toShortenFQNames="true" value="if $1$, do: $END$">
    <variable alwaysStopAt="true" defaultValue="&quot;condition&quot;" expression="" name="1"/>
    <context>
      <option name="ELIXIR" value="true"/>
    </context>
  </template>
  <template description="" name="ife if .. do .. else .. end" toReformat="true" toShortenFQNames="true" value="if $1$ do$2$else$END$end">
    <variable alwaysStopAt="true" defaultValue="&quot;condition&quot;" expression="" name="1"/>
    <context>
      <option name="ELIXIR" value="true"/>
    </context>
  </template>
  <template description="" name="ife: if .. do: .. else:" toReformat="true" toShortenFQNames="true" value="if $1$, do: $2$, else: $END$">
    <variable alwaysStopAt="true" defaultValue="&quot;condition&quot;" expression="" name="1"/>
    <context>
      <option name="ELIXIR" value="true"/>
    </context>
  </template>
  <template description="" name="unless unless .. do .. end" toReformat="true" toShortenFQNames="true" value="unless $1$ do$END$end">
    <context>
      <option name="ELIXIR" value="true"/>
    </context>
  </template>
  <template description="" name="unless: unless .. do: .." toReformat="true" toShortenFQNames="true" value="unless $1$, do: $END$">
    <variable alwaysStopAt="true" defaultValue="&quot;condition&quot;" expression="" name="1"/>
    <context>
      <option name="ELIXIR" value="true"/>
    </context>
  </template>
  <template description="" name="unlesse unless .. do .. else .. end" toReformat="true" toShortenFQNames="true" value="unless $1$ do$2$else$END$end">
    <variable alwaysStopAt="true" defaultValue="&quot;condition&quot;" expression="" name="1"/>
    <context>
      <option name="ELIXIR" value="true"/>
    </context>
  </template>
  <template description="" name="unlesse: unless .. do: .. else:" toReformat="true" toShortenFQNames="true" value="unless $1$, do: $2$, else: $END$">
    <variable alwaysStopAt="true" defaultValue="&quot;condition&quot;" expression="" name="1"/>
    <context>
      <option name="ELIXIR" value="true"/>
    </context>
  </template>
  <template description="" name="cond" toReformat="true" toShortenFQNames="true" value="cond do$1$ -&gt;$END$end">
    <context>
      <option name="ELIXIR" value="true"/>
    </context>
  </template>
  <template description="" name="case" toReformat="true" toShortenFQNames="true" value="case $1$ do$2$ -&gt;$END$end">
    <context>
      <option name="ELIXIR" value="true"/>
    </context>
  </template>
  <template description="" name="for" toReformat="true" toShortenFQNames="true" value="for $1$ &lt;- $2$ do$END$end">
    <variable alwaysStopAt="true" defaultValue="&quot;item&quot;" expression="" name="1"/>
    <variable alwaysStopAt="true" defaultValue="&quot;items&quot;" expression="" name="2"/>
    <context>
      <option name="ELIXIR" value="true"/>
    </context>
  </template>
  <template description="" name="fori" toReformat="true" toShortenFQNames="true" value="for $1$ &lt;- $2$, into: $3$ do$END$end">
    <variable alwaysStopAt="true" defaultValue="&quot;item&quot;" expression="" name="1"/>
    <variable alwaysStopAt="true" defaultValue="&quot;items&quot;" expression="" name="2"/>
    <context>
      <option name="ELIXIR" value="true"/>
    </context>
  </template>
  <template description="" name="wi" toReformat="true" toShortenFQNames="true" value="with($1$ &lt;- $2$) do$END$end">
    <variable alwaysStopAt="true" defaultValue="&quot;item&quot;" expression="" name="1"/>
    <variable alwaysStopAt="true" defaultValue="&quot;items&quot;" expression="" name="2"/>
    <context>
      <option name="ELIXIR" value="true"/>
    </context>
  </template>
  <template description="" name="wie" toReformat="true" toShortenFQNames="true" value="with($1$ &lt;- $2$) do$3$else$4$ -&gt;$END$end">
    <variable alwaysStopAt="true" defaultValue="&quot;item&quot;" expression="" name="1"/>
    <variable alwaysStopAt="true" defaultValue="&quot;items&quot;" expression="" name="2"/>
    <context>
      <option name="ELIXIR" value="true"/>
    </context>
  </template>
  <template description="" name="df" toReformat="true" toShortenFQNames="true" value="def $1$, do: $2$">
    <variable alwaysStopAt="true" defaultValue="&quot;name&quot;" expression="" name="1"/>
    <context>
      <option name="ELIXIR" value="true"/>
    </context>
  </template>
  <template description="" name="def" toReformat="true" toShortenFQNames="true" value="def $1$ do$END$end">
    <variable alwaysStopAt="true" defaultValue="&quot;name&quot;" expression="" name="1"/>
    <context>
      <option name="ELIXIR" value="true"/>
    </context>
  </template>
  <template description="" name="defd" toReformat="true" toShortenFQNames="true" value="@doc &quot;&quot;&quot;$1$&quot;&quot;&quot;def $2$ do$END$end">
    <variable alwaysStopAt="true" defaultValue="&quot;doc string&quot;" expression="" name="1"/>
    <variable alwaysStopAt="true" defaultValue="&quot;name&quot;" expression="" name="2"/>
    <context>
      <option name="ELIXIR" value="true"/>
    </context>
  </template>
  <template description="" name="defim" toReformat="true" toShortenFQNames="true" value="defimpl $1$, for: $2$ do$END$end">
    <variable alwaysStopAt="true" defaultValue="&quot;protocol_name&quot;" expression="" name="1"/>
    <variable alwaysStopAt="true" defaultValue="&quot;data_type&quot;" expression="" name="2"/>
    <context>
      <option name="ELIXIR" value="true"/>
    </context>
  </template>
  <template description="" name="defma" toReformat="true" toShortenFQNames="true" value="defmacro $1$ do$END$end">
    <variable alwaysStopAt="true" defaultValue="&quot;name&quot;" expression="" name="1"/>
    <context>
      <option name="ELIXIR" value="true"/>
    </context>
  </template>
  <template description="" name="defmo" toReformat="true" toShortenFQNames="true" value="defmodule $1$ do$END$end">
    <variable alwaysStopAt="true" defaultValue="&quot;`substitute(vim_snippets#Filename(), '\(_\|^\)\(.\)', '\u\2', 'g')`&quot;" expression="" name="1"/>
    <context>
      <option name="ELIXIR" value="true"/>
    </context>
  </template>
  <template description="" name="dfp" toReformat="true" toShortenFQNames="true" value="defp $1$, do: $2$">
    <variable alwaysStopAt="true" defaultValue="&quot;name&quot;" expression="" name="1"/>
    <context>
      <option name="ELIXIR" value="true"/>
    </context>
  </template>
  <template description="" name="defp" toReformat="true" toShortenFQNames="true" value="defp $1$ do$END$end">
    <variable alwaysStopAt="true" defaultValue="&quot;name&quot;" expression="" name="1"/>
    <context>
      <option name="ELIXIR" value="true"/>
    </context>
  </template>
  <template description="" name="defpr" toReformat="true" toShortenFQNames="true" value="defprotocol $1$, [$END$]">
    <variable alwaysStopAt="true" defaultValue="&quot;name&quot;" expression="" name="1"/>
    <variable alwaysStopAt="true" defaultValue="&quot;function&quot;" expression="" name="0"/>
    <context>
      <option name="ELIXIR" value="true"/>
    </context>
  </template>
  <template description="" name="defr" toReformat="true" toShortenFQNames="true" value="defrecord $1$, $END$">
    <variable alwaysStopAt="true" defaultValue="&quot;record_name&quot;" expression="" name="1"/>
    <variable alwaysStopAt="true" defaultValue="&quot;fields&quot;" expression="" name="0"/>
    <context>
      <option name="ELIXIR" value="true"/>
    </context>
  </template>
  <template description="" name="doc" toReformat="true" toShortenFQNames="true" value="@doc &quot;&quot;&quot;$END$&quot;&quot;&quot;">
    <context>
      <option name="ELIXIR" value="true"/>
    </context>
  </template>
  <template description="" name="docf" toReformat="true" toShortenFQNames="true" value="@doc false">
    <context>
      <option name="ELIXIR" value="true"/>
    </context>
  </template>
  <template description="" name="fn" toReformat="true" toShortenFQNames="true" value="fn $1$ -&gt; $END$ end">
    <variable alwaysStopAt="true" defaultValue="&quot;args&quot;" expression="" name="1"/>
    <context>
      <option name="ELIXIR" value="true"/>
    </context>
  </template>
  <template description="" name="mdoc" toReformat="true" toShortenFQNames="true" value="@moduledoc &quot;&quot;&quot;$END$&quot;&quot;&quot;">
    <context>
      <option name="ELIXIR" value="true"/>
    </context>
  </template>
  <template description="" name="mdocf" toReformat="true" toShortenFQNames="true" value="@moduledoc false">
    <context>
      <option name="ELIXIR" value="true"/>
    </context>
  </template>
  <template description="" name="rec" toReformat="true" toShortenFQNames="true" value="receive do$1$ -&gt;$END$end">
    <context>
      <option name="ELIXIR" value="true"/>
    </context>
  </template>
  <template description="" name="req" toReformat="true" toShortenFQNames="true" value="require $END$">
    <variable alwaysStopAt="true" defaultValue="&quot;module_name&quot;" expression="" name="0"/>
    <context>
      <option name="ELIXIR" value="true"/>
    </context>
  </template>
  <template description="" name="imp" toReformat="true" toShortenFQNames="true" value="import $END$">
    <variable alwaysStopAt="true" defaultValue="&quot;module_name&quot;" expression="" name="0"/>
    <context>
      <option name="ELIXIR" value="true"/>
    </context>
  </template>
  <template description="" name="ali" toReformat="true" toShortenFQNames="true" value="alias $END$">
