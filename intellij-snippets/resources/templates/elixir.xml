    <context>
      <option name="ELIXIR" value="true"/>
    </context>
  </template>
  <template description="" name="test" toReformat="true" toShortenFQNames="true" value="test &quot;$1$&quot; do$END$end">
    <variable alwaysStopAt="true" defaultValue="&quot;test name&quot;" expression="" name="1"/>
    <context>
      <option name="ELIXIR" value="true"/>
    </context>
  </template>
  <template description="" name="testa" toReformat="true" toShortenFQNames="true" value="test &quot;$1$&quot;, %{$2$} do$END$end">
    <variable alwaysStopAt="true" defaultValue="&quot;test_name&quot;" expression="" name="1"/>
