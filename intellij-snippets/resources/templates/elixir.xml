    <context>
      <option name="ELIXIR" value="true"/>
    </context>
  </template>
  <template description="" name="des" toReformat="true" toShortenFQNames="true" value="describe &quot;$1$&quot; do$END$end">
    <variable alwaysStopAt="true" defaultValue="&quot;test group subject&quot;" expression="" name="1"/>
    <context>
      <option name="ELIXIR" value="true"/>
    </context>
  </template>
  <template description="" name="exunit" toReformat="true" toShortenFQNames="true" value="defmodule $1$ douse ExUnit.Case, async: true$END$end">
    <variable alwaysStopAt="true" defaultValue="&quot;`substitute(vim_snippets#Filename(), '\(_\|^\)\(.\)', '\u\2', 'g')`&quot;" expression="" name="1"/>
    <context>
      <option name="ELIXIR" value="true"/>
    </context>
  </template>
  <template description="" name="try try .. rescue .. end" toReformat="true" toShortenFQNames="true" value="try do$1$rescue$2$ -&gt; $END$end">
    <context>
      <option name="ELIXIR" value="true"/>
    </context>
  </template>
  <template description="" name="pry" toReformat="true" toShortenFQNames="true" value="require IEx; IEx.pry$END$">
    <context>
      <option name="ELIXIR" value="true"/>
    </context>
  </template>
  <template description="" name="qu" toReformat="true" toShortenFQNames="true" value="quote do$1$end">
    <context>
      <option name="ELIXIR" value="true"/>
    </context>
  </template>
</templateSet>
