<templateSet group="jsp">
  <template description="" name="@page" toReformat="true" toShortenFQNames="true" value="&lt;%@page contentType=&quot;text/html&quot; pageEncoding=&quot;UTF-8&quot;%&gt;">
    <context>
      <option name="JSP" value="true"/>
    </context>
  </template>
  <template description="" name="jstl" toReformat="true" toShortenFQNames="true" value="&lt;%@ taglib uri=&quot;http://java.sun.com/jsp/jstl/core&quot; prefix=&quot;c&quot; %&gt;&lt;%@ taglib uri=&quot;http://java.sun.com/jsp/jstl/functions&quot; prefix=&quot;fn&quot; %&gt;">
    <context>
      <option name="JSP" value="true"/>
    </context>
  </template>
  <template description="" name="jstl:c" toReformat="true" toShortenFQNames="true" value="&lt;%@ taglib uri=&quot;http://java.sun.com/jsp/jstl/core&quot; prefix=&quot;c&quot; %&gt;">
    <context>
      <option name="JSP" value="true"/>
    </context>
  </template>
  <template description="" name="jstl:fn" toReformat="true" toShortenFQNames="true" value="&lt;%@ taglib uri=&quot;http://java.sun.com/jsp/jstl/functions&quot; prefix=&quot;fn&quot; %&gt;">
    <context>
      <option name="JSP" value="true"/>
    </context>
  </template>
  <template description="" name="cpath" toReformat="true" toShortenFQNames="true" value="${pageContext.request.contextPath}">
    <context>
      <option name="JSP" value="true"/>
    </context>
  </template>
  <template description="" name="cout" toReformat="true" toShortenFQNames="true" value="&lt;c:out value=&quot;$1$&quot; default=&quot;$END$&quot; /&gt;">
    <context>
      <option name="JSP" value="true"/>
    </context>
  </template>
  <template description="" name="cset" toReformat="true" toShortenFQNames="true" value="&lt;c:set var=&quot;$1$&quot; value=&quot;$END$&quot; /&gt;">
    <context>
      <option name="JSP" value="true"/>
    </context>
  </template>
  <template description="" name="cremove" toReformat="true" toShortenFQNames="true" value="&lt;c:remove var=&quot;$1$&quot; scope=&quot;$END$&quot; /&gt;">
    <variable alwaysStopAt="true" defaultValue="&quot;page&quot;" expression="" name="0"/>
    <context>
      <option name="JSP" value="true"/>
    </context>
  </template>
  <template description="" name="ccatch" toReformat="true" toShortenFQNames="true" value="&lt;c:catch var=&quot;$END$&quot; /&gt;">
    <context>
      <option name="JSP" value="true"/>
    </context>
  </template>
  <template description="" name="cif" toReformat="true" toShortenFQNames="true" value="&lt;c:if test=&quot;${$1$}&quot;&gt;$END$&lt;/c:if&gt;">
    <context>
      <option name="JSP" value="true"/>
    </context>
  </template>
  <template description="" name="cchoose" toReformat="true" toShortenFQNames="true" value="&lt;c:choose&gt;$END$&lt;/c:choose&gt;">
    <context>
      <option name="JSP" value="true"/>
    </context>
  </template>
  <template description="" name="cwhen" toReformat="true" toShortenFQNames="true" value="&lt;c:when test=&quot;${$1$}&quot;&gt;$END$&lt;/c:when&gt;">
    <context>
      <option name="JSP" value="true"/>
    </context>
  </template>
  <template description="" name="cother" toReformat="true" toShortenFQNames="true" value="&lt;c:otherwise&gt;$END$&lt;/c:otherwise&gt;">
    <context>
      <option name="JSP" value="true"/>
    </context>
  </template>
  <template description="" name="cfore" toReformat="true" toShortenFQNames="true" value="&lt;c:forEach items=&quot;${$1$}&quot; var=&quot;$2$&quot; varStatus=&quot;$3$&quot;&gt;$END$&lt;/c:forEach&gt;">
    <variable alwaysStopAt="true" defaultValue="&quot;&lt;c:out value=&quot;$2&quot; /&gt;&quot;" expression="" name="0"/>
    <context>
      <option name="JSP" value="true"/>
    </context>
  </template>
  <template description="" name="cfort" toReformat="true" toShortenFQNames="true" value="&lt;c:set var=&quot;$1$&quot;&gt;$2$&lt;/c:set&gt;&lt;c:forTokens var=&quot;$3$&quot; items=&quot;${$1}&quot; delims=&quot;$4$&quot;&gt;$END$&lt;/c:forTokens&gt;">
    <variable alwaysStopAt="true" defaultValue="&quot;item1,item2,item3&quot;" expression="" name="2"/>
    <variable alwaysStopAt="true" defaultValue="&quot;,&quot;" expression="" name="4"/>
    <variable alwaysStopAt="true" defaultValue="&quot;&lt;c:out value=&quot;$3&quot; /&gt;&quot;" expression="" name="0"/>
    <context>
      <option name="JSP" value="true"/>
    </context>
  </template>
  <template description="" name="cparam" toReformat="true" toShortenFQNames="true" value="&lt;c:param name=&quot;$1$&quot; value=&quot;$END$&quot; /&gt;">
    <context>
      <option name="JSP" value="true"/>
    </context>
  </template>
  <template description="" name="cparam+" toReformat="true" toShortenFQNames="true" value="&lt;c:param name=&quot;$1$&quot; value=&quot;$2$&quot; /&gt;cparam+$END$">
    <context>
      <option name="JSP" value="true"/>
    </context>
  </template>
  <template description="" name="cimport" toReformat="true" toShortenFQNames="true" value="&lt;c:import url=&quot;$1$&quot; /&gt;">
    <context>
      <option name="JSP" value="true"/>
    </context>
  </template>
  <template description="" name="cimport+" toReformat="true" toShortenFQNames="true" value="&lt;c:import url=&quot;$1$&quot;&gt;&lt;c:param name=&quot;$2$&quot; value=&quot;$3$&quot; /&gt;cparam+$END$&lt;/c:import&gt;">
    <context>
      <option name="JSP" value="true"/>
    </context>
  </template>
  <template description="" name="curl" toReformat="true" toShortenFQNames="true" value="&lt;c:url value=&quot;$1$&quot; var=&quot;$2$&quot; /&gt;&lt;a href=&quot;${$2}&quot;&gt;$END$&lt;/a&gt;">
    <context>
      <option name="JSP" value="true"/>
    </context>
  </template>
  <template description="" name="curl+" toReformat="true" toShortenFQNames="true" value="&lt;c:url value=&quot;$1$&quot; var=&quot;$2$&quot;&gt;&lt;c:param name=&quot;$4$&quot; value=&quot;$5$&quot; /&gt;cparam+$END$&lt;/c:url&gt;&lt;a href=&quot;${$2}&quot;&gt;$3$&lt;/a&gt;">
    <context>
      <option name="JSP" value="true"/>
    </context>
  </template>
  <template description="" name="credirect" toReformat="true" toShortenFQNames="true" value="&lt;c:redirect url=&quot;$END$&quot; /&gt;">
    <context>
      <option name="JSP" value="true"/>
    </context>
  </template>
  <template description="" name="contains" toReformat="true" toShortenFQNames="true" value="${fn:contains($1$, $END$)}">
    <variable alwaysStopAt="true" defaultValue="&quot;string&quot;" expression="" name="1"/>
    <variable alwaysStopAt="true" defaultValue="&quot;substr&quot;" expression="" name="0"/>
    <context>
      <option name="JSP" value="true"/>
    </context>
  </template>
  <template description="" name="contains:i" toReformat="true" toShortenFQNames="true" value="${fn:containsIgnoreCase($1$, $END$)}">
    <variable alwaysStopAt="true" defaultValue="&quot;string&quot;" expression="" name="1"/>
    <variable alwaysStopAt="true" defaultValue="&quot;substr&quot;" expression="" name="0"/>
    <context>
      <option name="JSP" value="true"/>
    </context>
  </template>
  <template description="" name="endswith" toReformat="true" toShortenFQNames="true" value="${fn:endsWith($1$, $END$)}">
    <variable alwaysStopAt="true" defaultValue="&quot;string&quot;" expression="" name="1"/>
    <variable alwaysStopAt="true" defaultValue="&quot;suffix&quot;" expression="" name="0"/>
    <context>
      <option name="JSP" value="true"/>
    </context>
  </template>
  <template description="" name="escape" toReformat="true" toShortenFQNames="true" value="${fn:escapeXml($END$)}">
    <variable alwaysStopAt="true" defaultValue="&quot;string&quot;" expression="" name="0"/>
    <context>
      <option name="JSP" value="true"/>
    </context>
  </template>
  <template description="" name="indexof" toReformat="true" toShortenFQNames="true" value="${fn:indexOf($1$, $END$)}">
    <variable alwaysStopAt="true" defaultValue="&quot;string&quot;" expression="" name="1"/>
    <variable alwaysStopAt="true" defaultValue="&quot;substr&quot;" expression="" name="0"/>
    <context>
      <option name="JSP" value="true"/>
    </context>
  </template>
  <template description="" name="join" toReformat="true" toShortenFQNames="true" value="${fn:join($1$, $END$)}">
    <variable alwaysStopAt="true" defaultValue="&quot;collection&quot;" expression="" name="1"/>
    <variable alwaysStopAt="true" defaultValue="&quot;delims&quot;" expression="" name="0"/>
    <context>
      <option name="JSP" value="true"/>
    </context>
  </template>
  <template description="" name="length" toReformat="true" toShortenFQNames="true" value="${fn:length($END$)}">
    <variable alwaysStopAt="true" defaultValue="&quot;collection_or_string&quot;" expression="" name="0"/>
    <context>
      <option name="JSP" value="true"/>
    </context>
  </template>
  <template description="" name="replace" toReformat="true" toShortenFQNames="true" value="${fn:replace($1$, $2$, $END$)}">
    <variable alwaysStopAt="true" defaultValue="&quot;string&quot;" expression="" name="1"/>
    <variable alwaysStopAt="true" defaultValue="&quot;substr&quot;" expression="" name="2"/>
    <variable alwaysStopAt="true" defaultValue="&quot;replace&quot;" expression="" name="0"/>
    <context>
      <option name="JSP" value="true"/>
    </context>
  </template>
  <template description="" name="split" toReformat="true" toShortenFQNames="true" value="${fn:split($1$, $END$)}">
    <variable alwaysStopAt="true" defaultValue="&quot;string&quot;" expression="" name="1"/>
    <variable alwaysStopAt="true" defaultValue="&quot;delims&quot;" expression="" name="0"/>
    <context>
      <option name="JSP" value="true"/>
    </context>
  </template>
  <template description="" name="startswith" toReformat="true" toShortenFQNames="true" value="${fn:startsWith($1$, $END$)}">
    <variable alwaysStopAt="true" defaultValue="&quot;string&quot;" expression="" name="1"/>
    <variable alwaysStopAt="true" defaultValue="&quot;prefix&quot;" expression="" name="0"/>
    <context>
      <option name="JSP" value="true"/>
    </context>
  </template>
  <template description="" name="substr" toReformat="true" toShortenFQNames="true" value="${fn:substring($1$, $2$, $END$)}">
    <variable alwaysStopAt="true" defaultValue="&quot;string&quot;" expression="" name="1"/>
    <variable alwaysStopAt="true" defaultValue="&quot;begin&quot;" expression="" name="2"/>
    <variable alwaysStopAt="true" defaultValue="&quot;end&quot;" expression="" name="0"/>
    <context>
      <option name="JSP" value="true"/>
    </context>
  </template>
  <template description="" name="substr:a" toReformat="true" toShortenFQNames="true" value="${fn:substringAfter($1$, $END$)}">
    <variable alwaysStopAt="true" defaultValue="&quot;string&quot;" expression="" name="1"/>
    <variable alwaysStopAt="true" defaultValue="&quot;substr&quot;" expression="" name="0"/>
    <context>
      <option name="JSP" value="true"/>
    </context>
  </template>
  <template description="" name="substr:b" toReformat="true" toShortenFQNames="true" value="${fn:substringBefore($1$, $END$)}">
    <variable alwaysStopAt="true" defaultValue="&quot;string&quot;" expression="" name="1"/>
    <variable alwaysStopAt="true" defaultValue="&quot;substr&quot;" expression="" name="0"/>
    <context>
      <option name="JSP" value="true"/>
    </context>
  </template>
  <template description="" name="lc" toReformat="true" toShortenFQNames="true" value="${fn:toLowerCase($END$)}">
    <variable alwaysStopAt="true" defaultValue="&quot;string&quot;" expression="" name="0"/>
    <context>
      <option name="JSP" value="true"/>
    </context>
  </template>
  <template description="" name="uc" toReformat="true" toShortenFQNames="true" value="${fn:toUpperCase($END$)}">
    <variable alwaysStopAt="true" defaultValue="&quot;string&quot;" expression="" name="0"/>
    <context>
      <option name="JSP" value="true"/>
    </context>
  </template>
</templateSet>
