      <option name="AUTOIT" value="true"/>
    </context>
  </template>
  <template description="" name="switch" toReformat="true" toShortenFQNames="true" value="Switch ($1$)Case $2$:$3$Case Else:$END$EndSwitch">
    <variable alwaysStopAt="true" defaultValue="&quot;condition&quot;" expression="" name="1"/>
    <variable alwaysStopAt="true" defaultValue="&quot;case1&quot;" expression="" name="2"/>
    <variable alwaysStopAt="true" defaultValue="&quot;; Case 1 code&quot;" expression="" name="3"/>
    <variable alwaysStopAt="true" defaultValue="&quot;; Else code&quot;" expression="" name="0"/>
    <context>
      <option name="AUTOIT" value="true"/>
    </context>
  </template>
  <template description="" name="select" toReformat="true" toShortenFQNames="true" value="Select ($1$)Case $2$:$3$Case Else:$END$EndSelect">
    <variable alwaysStopAt="true" defaultValue="&quot;condition&quot;" expression="" name="1"/>
    <variable alwaysStopAt="true" defaultValue="&quot;case1&quot;" expression="" name="2"/>
    <variable alwaysStopAt="true" defaultValue="&quot;; Case 1 code&quot;" expression="" name="3"/>
    <variable alwaysStopAt="true" defaultValue="&quot;; Else code&quot;" expression="" name="0"/>
    <context>
      <option name="AUTOIT" value="true"/>
    </context>
  </template>
  <template description="" name="wh" toReformat="true" toShortenFQNames="true" value="While ($1$)$END$WEnd">
    <variable alwaysStopAt="true" defaultValue="&quot;condition&quot;" expression="" name="1"/>
    <variable alwaysStopAt="true" defaultValue="&quot;; code...&quot;" expression="" name="0"/>
    <context>
      <option name="AUTOIT" value="true"/>
    </context>
  </template>
  <template description="" name="for" toReformat="true" toShortenFQNames="true" value="For $1$ = $3$ to $2$$END$Next">
    <variable alwaysStopAt="true" defaultValue="&quot;n&quot;" expression="" name="1"/>
    <variable alwaysStopAt="true" defaultValue="&quot;1&quot;" expression="" name="3"/>
    <variable alwaysStopAt="true" defaultValue="&quot;count&quot;" expression="" name="2"/>
    <variable alwaysStopAt="true" defaultValue="&quot;; code...&quot;" expression="" name="0"/>
    <context>
      <option name="AUTOIT" value="true"/>
    </context>
  </template>
  <template description="" name="func" toReformat="true" toShortenFQNames="true" value="Func $1$($2$):$END$EndFunc">
    <variable alwaysStopAt="true" defaultValue="&quot;fname&quot;" expression="" name="1"/>
    <variable alwaysStopAt="true" defaultValue="&quot;`indent('.') ? 'self' : ''`&quot;" expression="" name="2"/>
    <variable alwaysStopAt="true" defaultValue="&quot;Return&quot;" expression="" name="0"/>
    <context>
      <option name="AUTOIT" value="true"/>
    </context>
  </template>
  <template description="" name="msg" toReformat="true" toShortenFQNames="true" value="MsgBox($END$, $1$, $2$)">
    <variable alwaysStopAt="true" defaultValue="&quot;MsgType&quot;" expression="" name="0"/>
