<templateSet group="autoit">
  <template description="" name="if" toReformat="true" toShortenFQNames="true" value="If $1$ Then$END$EndIf">
    <variable alwaysStopAt="true" defaultValue="&quot;condition&quot;" expression="" name="1"/>
    <variable alwaysStopAt="true" defaultValue="&quot;; True code&quot;" expression="" name="0"/>
    <context>
      <option name="AUTOIT" value="true"/>
    </context>
  </template>
  <template description="" name="el" toReformat="true" toShortenFQNames="true" value="Else$END$">
    <context>
      <option name="AUTOIT" value="true"/>
    </context>
  </template>
  <template description="" name="eif" toReformat="true" toShortenFQNames="true" value="ElseIf $1$ Then$END$">
    <variable alwaysStopAt="true" defaultValue="&quot;condition&quot;" expression="" name="1"/>
    <variable alwaysStopAt="true" defaultValue="&quot;; True code&quot;" expression="" name="0"/>
    <context>
      <option name="AUTOIT" value="true"/>
    </context>
  </template>
  <template description="" name="ife" toReformat="true" toShortenFQNames="true" value="If $1$ Then$2$Else$END$EndIf">
