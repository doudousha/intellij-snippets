  <template description="" name="ifelif" toReformat="true" toShortenFQNames="true" value="If $1$ Then$2$ElseIf $3$ Then$4$Else$END$EndIf">
    <variable alwaysStopAt="true" defaultValue="&quot;condition 1&quot;" expression="" name="1"/>
    <variable alwaysStopAt="true" defaultValue="&quot;; True code&quot;" expression="" name="2"/>
    <variable alwaysStopAt="true" defaultValue="&quot;condition 2&quot;" expression="" name="3"/>
    <variable alwaysStopAt="true" defaultValue="&quot;; True code&quot;" expression="" name="4"/>
    <variable alwaysStopAt="true" defaultValue="&quot;; Else code&quot;" expression="" name="0"/>
