    <variable alwaysStopAt="true" defaultValue="&quot;&quot;Message Text&quot;&quot;" expression="" name="2"/>
    <context>
      <option name="AUTOIT" value="true"/>
    </context>
  </template>
  <template description="" name="debug" toReformat="true" toShortenFQNames="true" value="MsgBox(0, &quot;Debug&quot;, $END$)">
    <variable alwaysStopAt="true" defaultValue="&quot;&quot;Debug Message&quot;&quot;" expression="" name="0"/>
    <context>
      <option name="AUTOIT" value="true"/>
    </context>
  </template>
</templateSet>
