<templateSet group="lua">
  <template description="" name="#!" toReformat="true" toShortenFQNames="true" value="#!/usr/bin/env lua$1">
    <context>
      <option name="LUA" value="true"/>
    </context>
  </template>
  <template description="" name="local" toReformat="true" toShortenFQNames="true" value="local $1$ = $END$">
    <variable alwaysStopAt="true" defaultValue="&quot;x&quot;" expression="" name="1"/>
    <variable alwaysStopAt="true" defaultValue="&quot;1&quot;" expression="" name="0"/>
    <context>
      <option name="LUA" value="true"/>
    </context>
  </template>
  <template description="" name="fun" toReformat="true" toShortenFQNames="true" value="function $1$($2$)$END$end">
    <variable alwaysStopAt="true" defaultValue="&quot;fname&quot;" expression="" name="1"/>
    <variable alwaysStopAt="true" defaultValue="&quot;...&quot;" expression="" name="2"/>
    <variable alwaysStopAt="true" defaultValue="&quot;-- body&quot;" expression="" name="0"/>
    <context>
      <option name="LUA" value="true"/>
    </context>
  </template>
  <template description="" name="for" toReformat="true" toShortenFQNames="true" value="for $1$=$2$,$3$ do$END$end">
    <variable alwaysStopAt="true" defaultValue="&quot;i&quot;" expression="" name="1"/>
    <variable alwaysStopAt="true" defaultValue="&quot;1&quot;" expression="" name="2"/>
    <variable alwaysStopAt="true" defaultValue="&quot;10&quot;" expression="" name="3"/>
    <variable alwaysStopAt="true" defaultValue="&quot;print(i)&quot;" expression="" name="0"/>
    <context>
      <option name="LUA" value="true"/>
    </context>
  </template>
  <template description="" name="forp" toReformat="true" toShortenFQNames="true" value="for $1$,$2$ in pairs($3$) do$END$end">
    <variable alwaysStopAt="true" defaultValue="&quot;i&quot;" expression="" name="1"/>
    <variable alwaysStopAt="true" defaultValue="&quot;v&quot;" expression="" name="2"/>
    <variable alwaysStopAt="true" defaultValue="&quot;table_name&quot;" expression="" name="3"/>
    <variable alwaysStopAt="true" defaultValue="&quot;-- body&quot;" expression="" name="0"/>
    <context>
      <option name="LUA" value="true"/>
    </context>
  </template>
</templateSet>
