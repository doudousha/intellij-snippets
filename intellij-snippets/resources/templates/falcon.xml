<templateSet group="falcon">
  <template description="" name="#!" toReformat="true" toShortenFQNames="true" value="#!/usr/bin/env falcon">
    <context>
      <option name="FALCON" value="true"/>
    </context>
  </template>
  <template description="" name="imp" toReformat="true" toShortenFQNames="true" value="import $END$">
    <variable alwaysStopAt="true" defaultValue="&quot;module&quot;" expression="" name="0"/>
    <context>
      <option name="FALCON" value="true"/>
    </context>
  </template>
  <template description="" name="fun" toReformat="true" toShortenFQNames="true" value="function $2$($3$)$END$end">
    <variable alwaysStopAt="true" defaultValue="&quot;function_name&quot;" expression="" name="2"/>
    <context>
      <option name="FALCON" value="true"/>
    </context>
  </template>
  <template description="" name="class" toReformat="true" toShortenFQNames="true" value="class $1$($2$)$END$end">
    <variable alwaysStopAt="true" defaultValue="&quot;class_name&quot;" expression="" name="1"/>
    <variable alwaysStopAt="true" defaultValue="&quot;class_params&quot;" expression="" name="2"/>
    <variable alwaysStopAt="true" defaultValue="&quot;/* members/methods */&quot;" expression="" name="0"/>
    <context>
      <option name="FALCON" value="true"/>
    </context>
  </template>
  <template description="" name="if" toReformat="true" toShortenFQNames="true" value="if $1$$END$end">
    <variable alwaysStopAt="true" defaultValue="&quot;condition&quot;" expression="" name="1"/>
    <context>
      <option name="FALCON" value="true"/>
    </context>
  </template>
  <template description="" name="ife" toReformat="true" toShortenFQNames="true" value="if $1$$END$else$1$end">
    <variable alwaysStopAt="true" defaultValue="&quot;condition&quot;" expression="" name="1"/>
    <context>
      <option name="FALCON" value="true"/>
    </context>
  </template>
  <template description="" name="eif" toReformat="true" toShortenFQNames="true" value="elif $1$$END$">
    <variable alwaysStopAt="true" defaultValue="&quot;condition&quot;" expression="" name="1"/>
    <context>
      <option name="FALCON" value="true"/>
    </context>
  </template>
  <template description="" name="switch" toReformat="true" toShortenFQNames="true" value="switch $1$case $2$case $END$defaultend">
    <variable alwaysStopAt="true" defaultValue="&quot;expression&quot;" expression="" name="1"/>
    <variable alwaysStopAt="true" defaultValue="&quot;item&quot;" expression="" name="2"/>
    <variable alwaysStopAt="true" defaultValue="&quot;item&quot;" expression="" name="0"/>
    <context>
      <option name="FALCON" value="true"/>
    </context>
  </template>
  <template description="" name="select" toReformat="true" toShortenFQNames="true" value="select $1$case $2$case $END$defaultend">
    <variable alwaysStopAt="true" defaultValue="&quot;variable&quot;" expression="" name="1"/>
    <variable alwaysStopAt="true" defaultValue="&quot;TypeSpec&quot;" expression="" name="2"/>
    <variable alwaysStopAt="true" defaultValue="&quot;TypeSpec&quot;" expression="" name="0"/>
    <context>
      <option name="FALCON" value="true"/>
    </context>
  </template>
  <template description="" name="forin" toReformat="true" toShortenFQNames="true" value="for $1$ in $2$$END$end">
    <variable alwaysStopAt="true" defaultValue="&quot;element&quot;" expression="" name="1"/>
    <variable alwaysStopAt="true" defaultValue="&quot;container&quot;" expression="" name="2"/>
    <context>
      <option name="FALCON" value="true"/>
    </context>
  </template>
  <template description="" name="forto" toReformat="true" toShortenFQNames="true" value="for $1$ to $2$$END$end">
    <variable alwaysStopAt="true" defaultValue="&quot;lowerbound&quot;" expression="" name="1"/>
    <variable alwaysStopAt="true" defaultValue="&quot;upperbound&quot;" expression="" name="2"/>
    <context>
      <option name="FALCON" value="true"/>
    </context>
  </template>
</templateSet>
