<templateSet group="progress">
  <template description="" name="defbuf" toReformat="true" toShortenFQNames="true" value="DEFINE BUFFER b_$1$ FOR $1 $END$.">
    <variable alwaysStopAt="true" defaultValue="&quot;TableName&quot;" expression="" name="1"/>
    <context>
      <option name="PROGRESS" value="true"/>
    </context>
  </template>
  <template description="" name="defvar" toReformat="true" toShortenFQNames="true" value="DEFINE VARIABLE $1$ AS $END$.">
    <variable alwaysStopAt="true" defaultValue="&quot;VariableName&quot;" expression="" name="1"/>
    <context>
      <option name="PROGRESS" value="true"/>
    </context>
  </template>
  <template description="" name="nl" toReformat="true" toShortenFQNames="true" value="NO-LOCK">
    <context>
      <option name="PROGRESS" value="true"/>
    </context>
  </template>
  <template description="" name="ne" toReformat="true" toShortenFQNames="true" value="NO-ERROR">
    <context>
      <option name="PROGRESS" value="true"/>
    </context>
  </template>
  <template description="" name="nle" toReformat="true" toShortenFQNames="true" value="NO-LOCK NO-ERROR">
    <context>
      <option name="PROGRESS" value="true"/>
    </context>
  </template>
  <template description="" name="ini" toReformat="true" toShortenFQNames="true" value="INITIAL $END$">
    <variable alwaysStopAt="true" defaultValue="&quot;?&quot;" expression="" name="0"/>
    <context>
      <option name="PROGRESS" value="true"/>
    </context>
  </template>
  <template description="" name="nu" toReformat="true" toShortenFQNames="true" value="NO-UNDO">
    <context>
      <option name="PROGRESS" value="true"/>
    </context>
  </template>
  <template description="" name="err" toReformat="true" toShortenFQNames="true" value="ERROR">
    <context>
      <option name="PROGRESS" value="true"/>
    </context>
  </template>
  <template description="" name="ff" toReformat="true" toShortenFQNames="true" value="FIND FIRST $1$$2$} $END$">
    <variable alwaysStopAt="true" defaultValue="&quot;BufferName&quot;" expression="" name="1"/>
    <variable alwaysStopAt="true" defaultValue="&quot;WHERE $1.${3&quot;" expression="" name="2"/>
    <context>
      <option name="PROGRESS" value="true"/>
    </context>
  </template>
  <template description="" name="input" toReformat="true" toShortenFQNames="true" value="DEFINE INPUT PARAMETER $1$ AS $END$.">
    <variable alwaysStopAt="true" defaultValue="&quot;ParamName&quot;" expression="" name="1"/>
    <context>
      <option name="PROGRESS" value="true"/>
    </context>
  </template>
  <template description="" name="output" toReformat="true" toShortenFQNames="true" value="DEFINE OUTPUT PARAMETER $1$ AS $END$.">
    <variable alwaysStopAt="true" defaultValue="&quot;ParamName&quot;" expression="" name="1"/>
    <variable alwaysStopAt="true" defaultValue="&quot;ParamType&quot;" expression="" name="0"/>
    <context>
      <option name="PROGRESS" value="true"/>
    </context>
  </template>
  <template description="" name="proc" toReformat="true" toShortenFQNames="true" value="/******************************************************************************/PROCEDURE $1$:$END$END PROCEDURE. /* $1 *//******************************************************************************/">
    <variable alwaysStopAt="true" defaultValue="&quot;ProcName&quot;" expression="" name="1"/>
    <context>
      <option name="PROGRESS" value="true"/>
    </context>
  </template>
  <template description="" name="alert" toReformat="true" toShortenFQNames="true" value="MESSAGE &quot;$1$&quot; $2$ VIEW-AS ALERT-BOX.">
    <variable alwaysStopAt="true" defaultValue="&quot;MessageContent&quot;" expression="" name="1"/>
    <variable alwaysStopAt="true" defaultValue="&quot;Data&quot;" expression="" name="2"/>
    <context>
      <option name="PROGRESS" value="true"/>
    </context>
  </template>
  <template description="" name="if" toReformat="true" toShortenFQNames="true" value="IF $1$THEN $2$$3$}">
    <variable alwaysStopAt="true" defaultValue="&quot;Condition&quot;" expression="" name="1"/>
    <variable alwaysStopAt="true" defaultValue="&quot;Action&quot;" expression="" name="2"/>
    <variable alwaysStopAt="true" defaultValue="&quot;ELSE ${4:OtherWise&quot;" expression="" name="3"/>
    <context>
      <option name="PROGRESS" value="true"/>
    </context>
  </template>
  <template description="" name="do" toReformat="true" toShortenFQNames="true" value="DO$1$:$END$END.">
    <variable alwaysStopAt="true" defaultValue="&quot; Clauses&quot;" expression="" name="1"/>
    <context>
      <option name="PROGRESS" value="true"/>
    </context>
  </template>
  <template description="" name="int" toReformat="true" toShortenFQNames="true" value="INTEGER">
    <context>
      <option name="PROGRESS" value="true"/>
    </context>
  </template>
  <template description="" name="char" toReformat="true" toShortenFQNames="true" value="CHARACTER">
    <context>
      <option name="PROGRESS" value="true"/>
    </context>
  </template>
  <template description="" name="log" toReformat="true" toShortenFQNames="true" value="LOGICAL">
    <context>
      <option name="PROGRESS" value="true"/>
    </context>
  </template>
  <template description="" name="dec" toReformat="true" toShortenFQNames="true" value="DECIMAL">
    <context>
      <option name="PROGRESS" value="true"/>
    </context>
  </template>
</templateSet>
