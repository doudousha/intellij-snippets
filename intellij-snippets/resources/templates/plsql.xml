<templateSet group="plsql">
  <template description="" name="ps" toReformat="true" toShortenFQNames="true" value="create or replace package $1$as$END$end; -- end of package spec $1">
    <variable alwaysStopAt="true" defaultValue="&quot;name&quot;" expression="" name="1"/>
    <variable alwaysStopAt="true" defaultValue="&quot;-- spec&quot;" expression="" name="0"/>
    <context>
      <option name="PLSQL" value="true"/>
    </context>
  </template>
  <template description="" name="pb" toReformat="true" toShortenFQNames="true" value="create or replace package body $1$as$END$end; -- end of package body $1;">
    <variable alwaysStopAt="true" defaultValue="&quot;name&quot;" expression="" name="1"/>
    <variable alwaysStopAt="true" defaultValue="&quot;-- body&quot;" expression="" name="0"/>
    <context>
      <option name="PLSQL" value="true"/>
    </context>
  </template>
  <template description="" name="pps" toReformat="true" toShortenFQNames="true" value="procedure $1$($END$);">
    <variable alwaysStopAt="true" defaultValue="&quot;name&quot;" expression="" name="1"/>
    <variable alwaysStopAt="true" defaultValue="&quot;args&quot;" expression="" name="0"/>
    <context>
      <option name="PLSQL" value="true"/>
    </context>
  </template>
  <template description="" name="ppb" toReformat="true" toShortenFQNames="true" value="procedure $1$($2$)asbegin$END$end $2;">
    <variable alwaysStopAt="true" defaultValue="&quot;name&quot;" expression="" name="1"/>
    <variable alwaysStopAt="true" defaultValue="&quot;args&quot;" expression="" name="2"/>
    <variable alwaysStopAt="true" defaultValue="&quot;-- body&quot;" expression="" name="0"/>
    <context>
      <option name="PLSQL" value="true"/>
    </context>
  </template>
  <template description="" name="pfs" toReformat="true" toShortenFQNames="true" value="function $1$($2$)return $END$;">
    <variable alwaysStopAt="true" defaultValue="&quot;name&quot;" expression="" name="1"/>
    <variable alwaysStopAt="true" defaultValue="&quot;args&quot;" expression="" name="2"/>
    <variable alwaysStopAt="true" defaultValue="&quot;type&quot;" expression="" name="0"/>
    <context>
      <option name="PLSQL" value="true"/>
    </context>
  </template>
  <template description="" name="pfb" toReformat="true" toShortenFQNames="true" value="function $1$($2$)return $3$asl_res&#9;$3;begin$END$;return l_res;end $1;">
    <variable alwaysStopAt="true" defaultValue="&quot;name&quot;" expression="" name="1"/>
    <variable alwaysStopAt="true" defaultValue="&quot;args&quot;" expression="" name="2"/>
    <variable alwaysStopAt="true" defaultValue="&quot;type&quot;" expression="" name="3"/>
    <variable alwaysStopAt="true" defaultValue="&quot;-- body&quot;" expression="" name="0"/>
    <context>
      <option name="PLSQL" value="true"/>
    </context>
  </template>
  <template description="" name="err" toReformat="true" toShortenFQNames="true" value="show errors;">
    <context>
      <option name="PLSQL" value="true"/>
    </context>
  </template>
  <template description="" name="p" toReformat="true" toShortenFQNames="true" value="$1$ $2$ $3$ $END$">
    <variable alwaysStopAt="true" defaultValue="&quot;name&quot;" expression="" name="1"/>
    <variable alwaysStopAt="true" defaultValue="&quot;in&quot;" expression="" name="2"/>
    <variable alwaysStopAt="true" defaultValue="&quot;type&quot;" expression="" name="3"/>
    <variable alwaysStopAt="true" defaultValue="&quot; := null&quot;" expression="" name="0"/>
    <context>
      <option name="PLSQL" value="true"/>
    </context>
  </template>
  <template description="" name="tr" toReformat="true" toShortenFQNames="true" value="type tr_$1$ is record ($END$);">
    <variable alwaysStopAt="true" defaultValue="&quot;name&quot;" expression="" name="1"/>
    <variable alwaysStopAt="true" defaultValue="&quot;/* columns */&quot;" expression="" name="0"/>
    <context>
      <option name="PLSQL" value="true"/>
    </context>
  </template>
  <template description="" name="tt" toReformat="true" toShortenFQNames="true" value="type tt_$1$ is table of tr_$END$;">
    <variable alwaysStopAt="true" defaultValue="&quot;name&quot;" expression="" name="1"/>
    <variable alwaysStopAt="true" defaultValue="&quot;name&quot;" expression="" name="0"/>
    <context>
      <option name="PLSQL" value="true"/>
    </context>
  </template>
  <template description="" name="tti" toReformat="true" toShortenFQNames="true" value="type tt_$1$ is table of tr_$END$ index by binary_integer;">
    <variable alwaysStopAt="true" defaultValue="&quot;name&quot;" expression="" name="1"/>
    <variable alwaysStopAt="true" defaultValue="&quot;name&quot;" expression="" name="0"/>
    <context>
      <option name="PLSQL" value="true"/>
    </context>
  </template>
  <template description="" name="doc" toReformat="true" toShortenFQNames="true" value="/**&#9;$END$*/">
    <variable alwaysStopAt="true" defaultValue="&quot; comment ...&quot;" expression="" name="0"/>
    <context>
      <option name="PLSQL" value="true"/>
    </context>
  </template>
  <template description="" name="beg" toReformat="true" toShortenFQNames="true" value="begin$END$end;">
    <context>
      <option name="PLSQL" value="true"/>
    </context>
  </template>
  <template description="" name="dec" toReformat="true" toShortenFQNames="true" value="declare$1$begin$END$end;">
    <context>
      <option name="PLSQL" value="true"/>
    </context>
  </template>
  <template description="" name="rpipe" toReformat="true" toShortenFQNames="true" value="for $1$ in 1 .. $END$.count looppipe row( $2($1) );end loop;return;">
    <variable alwaysStopAt="true" defaultValue="&quot;i&quot;" expression="" name="1"/>
    <variable alwaysStopAt="true" defaultValue="&quot;l_res&quot;" expression="" name="0"/>
    <context>
      <option name="PLSQL" value="true"/>
    </context>
  </template>
  <template description="" name="bc" toReformat="true" toShortenFQNames="true" value="bulk collect into $END$">
    <context>
      <option name="PLSQL" value="true"/>
    </context>
  </template>
  <template description="" name="l" toReformat="true" toShortenFQNames="true" value="l_$1$&#9;&#9;$END$;">
    <variable alwaysStopAt="true" defaultValue="&quot;number&quot;" expression="" name="0"/>
    <context>
      <option name="PLSQL" value="true"/>
    </context>
  </template>
  <template description="" name="log" toReformat="true" toShortenFQNames="true" value="dbms_output.put_line('$END$');">
    <context>
      <option name="PLSQL" value="true"/>
    </context>
  </template>
  <template description="" name="for" toReformat="true" toShortenFQNames="true" value="for $1$ in $2$..$3$ loop$END$end loop;">
    <variable alwaysStopAt="true" defaultValue="&quot;i&quot;" expression="" name="1"/>
    <variable alwaysStopAt="true" defaultValue="&quot;1&quot;" expression="" name="2"/>
    <variable alwaysStopAt="true" defaultValue="&quot;42&quot;" expression="" name="3"/>
    <context>
      <option name="PLSQL" value="true"/>
    </context>
  </template>
  <template description="" name="fors" toReformat="true" toShortenFQNames="true" value="for $1$ in ($2$) loop$END$end loop;">
    <variable alwaysStopAt="true" defaultValue="&quot;rec&quot;" expression="" name="1"/>
    <variable alwaysStopAt="true" defaultValue="&quot; select&quot;" expression="" name="2"/>
    <context>
      <option name="PLSQL" value="true"/>
    </context>
  </template>
  <template description="" name="forc" toReformat="true" toShortenFQNames="true" value="for $1$ in $2$.first .. $2.last loop$END$end loop;">
    <variable alwaysStopAt="true" defaultValue="&quot;i&quot;" expression="" name="1"/>
    <variable alwaysStopAt="true" defaultValue="&quot;l_var&quot;" expression="" name="2"/>
    <variable alwaysStopAt="true" defaultValue="&quot; -- dbms_output.put_line($2($1)); &quot;" expression="" name="0"/>
    <context>
      <option name="PLSQL" value="true"/>
    </context>
  </template>
  <template description="" name="if" toReformat="true" toShortenFQNames="true" value="if $1$ then$END$end if;">
    <context>
      <option name="PLSQL" value="true"/>
    </context>
  </template>
</templateSet>
