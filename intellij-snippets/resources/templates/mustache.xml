<templateSet group="mustache">
  <template description="" name="if # {{#value}} ... {{/value}}" toReformat="true" toShortenFQNames="true" value="{{#$1$}}$END${{/$1}}">
    <variable alwaysStopAt="true" defaultValue="&quot;value&quot;" expression="" name="1"/>
    <context>
      <option name="MUSTACHE" value="true"/>
    </context>
  </template>
  <template description="" name="ifn # {{^value}} ... {{/value}}" toReformat="true" toShortenFQNames="true" value="{{^$1$}}$END${{/$1}}">
    <variable alwaysStopAt="true" defaultValue="&quot;value&quot;" expression="" name="1"/>
    <context>
      <option name="MUSTACHE" value="true"/>
    </context>
  </template>
</templateSet>
