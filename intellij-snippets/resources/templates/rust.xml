<templateSet group="rust">
  <template description="" name="fn" toReformat="true" toShortenFQNames="true" value="fn $1$($2$)$3$ {$END$}">
    <variable alwaysStopAt="true" defaultValue="&quot;function_name&quot;" expression="" name="1"/>
    <context>
      <option name="RUST" value="true"/>
    </context>
  </template>
  <template description="" name="pfn" toReformat="true" toShortenFQNames="true" value="pub fn $1$($2$)$3$ {$END$}">
