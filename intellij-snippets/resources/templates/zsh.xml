<templateSet group="zsh">
  <template description="" name="#!" toReformat="true" toShortenFQNames="true" value="#!/bin/zsh">
    <context>
      <option name="ZSH" value="true"/>
    </context>
  </template>
  <template description="" name="if" toReformat="true" toShortenFQNames="true" value="if $1$; then$END$fi">
    <variable alwaysStopAt="true" defaultValue="&quot;condition&quot;" expression="" name="1"/>
    <variable alwaysStopAt="true" defaultValue="&quot;# statements&quot;" expression="" name="0"/>
    <context>
      <option name="ZSH" value="true"/>
    </context>
  </template>
  <template description="" name="ife" toReformat="true" toShortenFQNames="true" value="if $1$; then$2$else$END$fi">
    <variable alwaysStopAt="true" defaultValue="&quot;condition&quot;" expression="" name="1"/>
    <variable alwaysStopAt="true" defaultValue="&quot;# statements&quot;" expression="" name="2"/>
    <variable alwaysStopAt="true" defaultValue="&quot;# statements&quot;" expression="" name="0"/>
    <context>
      <option name="ZSH" value="true"/>
    </context>
  </template>
  <template description="" name="eif" toReformat="true" toShortenFQNames="true" value="elif $1$; then$END$">
    <variable alwaysStopAt="true" defaultValue="&quot;condition&quot;" expression="" name="1"/>
    <variable alwaysStopAt="true" defaultValue="&quot;# statements&quot;" expression="" name="0"/>
    <context>
      <option name="ZSH" value="true"/>
    </context>
  </template>
  <template description="" name="for" toReformat="true" toShortenFQNames="true" value="for (( $2$ = 0; $2 &lt; $1$; $2++ )); do$END$done">
    <variable alwaysStopAt="true" defaultValue="&quot;i&quot;" expression="" name="2"/>
    <variable alwaysStopAt="true" defaultValue="&quot;count&quot;" expression="" name="1"/>
    <variable alwaysStopAt="true" defaultValue="&quot;# statements&quot;" expression="" name="0"/>
    <context>
      <option name="ZSH" value="true"/>
    </context>
  </template>
  <template description="" name="fori" toReformat="true" toShortenFQNames="true" value="for $1$ in $2$; do$END$done">
    <variable alwaysStopAt="true" defaultValue="&quot;needle&quot;" expression="" name="1"/>
    <variable alwaysStopAt="true" defaultValue="&quot;haystack&quot;" expression="" name="2"/>
    <variable alwaysStopAt="true" defaultValue="&quot;#statements&quot;" expression="" name="0"/>
    <context>
      <option name="ZSH" value="true"/>
    </context>
  </template>
  <template description="" name="fore" toReformat="true" toShortenFQNames="true" value="for $1$ in $2$; do$END$done">
    <variable alwaysStopAt="true" defaultValue="&quot;item&quot;" expression="" name="1"/>
    <variable alwaysStopAt="true" defaultValue="&quot;list&quot;" expression="" name="2"/>
    <variable alwaysStopAt="true" defaultValue="&quot;# statements&quot;" expression="" name="0"/>
    <context>
      <option name="ZSH" value="true"/>
    </context>
  </template>
  <template description="" name="wh" toReformat="true" toShortenFQNames="true" value="while $1$; do$END$done">
    <variable alwaysStopAt="true" defaultValue="&quot;condition&quot;" expression="" name="1"/>
    <variable alwaysStopAt="true" defaultValue="&quot;# statements&quot;" expression="" name="0"/>
    <context>
      <option name="ZSH" value="true"/>
    </context>
  </template>
  <template description="" name="until" toReformat="true" toShortenFQNames="true" value="until $1$; do$END$done">
    <variable alwaysStopAt="true" defaultValue="&quot;condition&quot;" expression="" name="1"/>
    <variable alwaysStopAt="true" defaultValue="&quot;# statements&quot;" expression="" name="0"/>
    <context>
      <option name="ZSH" value="true"/>
    </context>
  </template>
  <template description="" name="repeat" toReformat="true" toShortenFQNames="true" value="repeat $1$; do$END$done">
    <variable alwaysStopAt="true" defaultValue="&quot;integer&quot;" expression="" name="1"/>
    <variable alwaysStopAt="true" defaultValue="&quot;# statements&quot;" expression="" name="0"/>
    <context>
      <option name="ZSH" value="true"/>
    </context>
  </template>
  <template description="" name="case" toReformat="true" toShortenFQNames="true" value="case $1$ in$2$)$END$;;esac">
    <variable alwaysStopAt="true" defaultValue="&quot;word&quot;" expression="" name="1"/>
    <variable alwaysStopAt="true" defaultValue="&quot;pattern&quot;" expression="" name="2"/>
    <context>
      <option name="ZSH" value="true"/>
    </context>
  </template>
  <template description="" name="select" toReformat="true" toShortenFQNames="true" value="select $1$ in $2$; do$END$done">
    <variable alwaysStopAt="true" defaultValue="&quot;answer&quot;" expression="" name="1"/>
    <variable alwaysStopAt="true" defaultValue="&quot;choices&quot;" expression="" name="2"/>
    <variable alwaysStopAt="true" defaultValue="&quot;# statements&quot;" expression="" name="0"/>
    <context>
      <option name="ZSH" value="true"/>
    </context>
  </template>
  <template description="" name="(" toReformat="true" toShortenFQNames="true" value="( $END$ )">
    <variable alwaysStopAt="true" defaultValue="&quot;#statements&quot;" expression="" name="0"/>
    <context>
      <option name="ZSH" value="true"/>
    </context>
  </template>
  <template description="" name="{" toReformat="true" toShortenFQNames="true" value="{ $END$ }">
    <variable alwaysStopAt="true" defaultValue="&quot;#statements&quot;" expression="" name="0"/>
    <context>
      <option name="ZSH" value="true"/>
    </context>
  </template>
  <template description="" name="[" toReformat="true" toShortenFQNames="true" value="[[ $END$ ]]">
    <variable alwaysStopAt="true" defaultValue="&quot;test&quot;" expression="" name="0"/>
    <context>
      <option name="ZSH" value="true"/>
    </context>
  </template>
  <template description="" name="always" toReformat="true" toShortenFQNames="true" value="{ $1$ } always { $END$ }">
    <variable alwaysStopAt="true" defaultValue="&quot;try&quot;" expression="" name="1"/>
    <variable alwaysStopAt="true" defaultValue="&quot;always&quot;" expression="" name="0"/>
    <context>
      <option name="ZSH" value="true"/>
    </context>
  </template>
  <template description="" name="fun" toReformat="true" toShortenFQNames="true" value="$1$() {$END$}">
    <variable alwaysStopAt="true" defaultValue="&quot;function_name&quot;" expression="" name="1"/>
    <variable alwaysStopAt="true" defaultValue="&quot;# function_body&quot;" expression="" name="0"/>
    <context>
      <option name="ZSH" value="true"/>
    </context>
  </template>
</templateSet>
